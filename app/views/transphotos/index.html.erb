<main class="main">
  <div class="inner">
    <% if user_signed_in? %>
      <div class="greeting">
        こんにちは、
        <%= link_to "#{current_user.nickname}さん", user_path(current_user), class: :greeting__link%>
      </div>
    <% end %>
    <div class="card__wrapper">
      <div class="transphotos">
        <%= render partial: 'transphoto', collection: @transphotos %>
      </div>
    </div>

  </div>
</main>